<h5>@ViewBag.name</h5>
<ul>
    @foreach (Usuario user in ViewBag.usuarios)
    {
        <li class = "userOrbit">
            <img src='@user.foto' alt="foto user">
            <a href=''>@user.nombre - @user.ubicacion</a>
        </li>
    }
</ul>
